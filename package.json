{
   "name": "growmatik-sample",
   "private": true,
   "scripts": {
      "prepare": "husky install",
      "type": "tsc",
      "build": "yarn type && cross-env NODE_ENV=production webpack",
      "build-dev": "webpack --watch",
      "serve": "opener http://localhost:5000 && serve -s build -l 5000",
      "prod": "yarn build && yarn serve",
      "analyze": "cross-env ANALYZE=true yarn build && webpack-bundle-analyzer build/stats.json",
      "dev": "cross-env NODE_ENV=development webpack serve --open",
      "test": "jest",
      "tdd": "jest --watch",
      "coverage": "jest --coverage",
      "format": "prettier --write"
   },
   "lint-staged": {
      "src/**/*.{ts,tsx.scss,html}": "yarn format",
      "*.{js,json,md}": "yarn format"
   },
   "dependencies": {
      "core-js": "^3.11.0",
      "react": "^17.0.2",
      "react-dom": "^17.0.2"
   },
   "devDependencies": {
      "@babel/core": "^7.13.16",
      "@babel/preset-env": "^7.13.15",
      "@babel/preset-react": "^7.13.13",
      "@babel/preset-typescript": "^7.13.0",
      "@testing-library/jest-dom": "^5.12.0",
      "@testing-library/react": "^11.2.6",
      "@testing-library/user-event": "^13.1.5",
      "@types/jest": "^26.0.22",
      "@types/react": "^17.0.3",
      "@types/react-dom": "^17.0.3",
      "@types/react-test-renderer": "^17.0.1",
      "babel-loader": "^8.2.2",
      "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
      "cross-env": "^7.0.3",
      "css-loader": "^5.2.4",
      "css-minimizer-webpack-plugin": "^2.0.0",
      "html-webpack-plugin": "^5.3.1",
      "husky": "^6.0.0",
      "identity-obj-proxy": "^3.0.0",
      "jest": "^26.6.3",
      "lint-staged": "^10.5.4",
      "mini-css-extract-plugin": "^1.5.0",
      "opener": "^1.5.2",
      "postcss": "^8.2.10",
      "postcss-loader": "^5.2.0",
      "postcss-preset-env": "^6.7.0",
      "prettier": "^2.2.1",
      "react-test-renderer": "^17.0.2",
      "sass": "^1.32.11",
      "sass-loader": "^11.0.1",
      "serve": "^11.3.2",
      "style-loader": "^2.0.0",
      "typescript": "^4.2.4",
      "webpack": "^5.35.0",
      "webpack-bundle-analyzer": "^4.4.1",
      "webpack-cli": "^4.6.0",
      "webpack-dev-server": "^3.11.2",
      "yarn": "^1.22.10"
   }
}
